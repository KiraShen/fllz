<view class="page">
    <view class="page__bd">
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd" style="color: #999">证书编号:</view>
                <view class="weui-cell__ft" style="color: #000">{{certID}}</view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd" style="color: #999">股票数量:</view>
                <view class="weui-cell__ft" style="color: #000">{{shares}}</view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd" style="color: #999">证书持有人:</view>
                <view class="weui-cell__ft" style="color: #000">{{name}}</view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd" style="color: #999">身份证:</view>
                <view class="weui-cell__ft" style="color: #000;font-size: 14px">{{idcard}}</view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd" style="color: #999">代理公司:</view>
                <view class="weui-cell__ft" style="color: #000;font-size: 14px">{{agent}}</view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd" style="color: #999">证书生效期:</view>
                <view class="weui-cell__ft" style="color: #000">{{time}}</view>
            </view>
        </view>
	</view>
</view>