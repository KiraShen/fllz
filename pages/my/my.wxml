<view class="page">
    <view class="page__bd">
        <view class="userInfo">
            <image src='{{avatarUrl}}' mode='widthFix' display="none"></image>
            <view>
                <button class="weui-btn mini-btn" size="mini" type="primary" plain="true"  hidden="{{handleStatus}}" open-type="getUserInfo" bindgetuserinfo="userInfoHandler">点击按钮获取头像
                </button>         
                <text hidden="{{!handleStatus}}">{{nickName}}</text>
            </view>        
        </view>

        <view class="weui-cells weui-cells_after-title">
            <navigator url="{{urlInfo}}" open-type="navigate"  class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="{{iconInfo}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">我的信息</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <navigator url="{{urlbinding}}" open-type="navigate"  class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="{{iconBinding}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">我的绑定</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <navigator url="{{urlCard}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="{{iconEmoji}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">我的会员卡</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <navigator url="{{urlOrder}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="{{iconOrder}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">我的订单</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <!-- <navigator url="{{urlFeedback}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="{{iconMsg}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">我的反馈</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator> 

            <navigator url="{{urlSet}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">            
                <view class="weui-cell__hd">
                    <image src="{{iconSet}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">设置</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator> -->
        </view>
    </view>
</view>